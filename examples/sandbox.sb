struct Point {
    x: int
    y: int
}

struct Rectangle {
    origin: Point
    width: int
    height: int
}

fn main(): int {
    let rect = new Rectangle {
        origin: new Point {
            x: 10
            y: 20
        }
        width: 100
        height: 50
    }
    rect.origin.x == 10
    rect.origin.x += 5
    rect.origin.x == 15

    return 0
}
