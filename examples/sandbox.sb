struct Point {
    x: int
    y: int
}

struct Rectangle {
    origin: Point
    width: int
    height: int
}

fn test_nested_field_access() {
    let rect = new Rectangle {
        origin: new Point {
            x: 10
            y: 20
        }
        width: 100
        height: 50
    }
    assert(rect.origin.x == 10)
    rect.origin.x += 5
    assert(rect.origin.x == 15)
}